for i in range(0,50,1):
    print(i)    #Using loop that counts up from 0 to 50 in increments of 1
for i in range(0,50,-1):
    print(i)    #Using  loop that counts down from 50 to 0 in decrements of 1.
for i in range(30,50,1):
    print(i)    #Using loop that counts up from 30 to 50 in increments of 1.
for i in range(50,10,-2):
    print(i)     #Using loop that counts down from 50 to 10 in decrements of 2.
for i in range(100,200,5):
    print(i)    #Using  loop that counts up from 100 to 200 in increments of 5.
